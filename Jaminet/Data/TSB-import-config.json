{
  "version": "1.0.0",
  "rules": [
    {
      "operation": "item-disable",
      "conditions": [
        {
          "element": "SHOP/SHOPITEM/STOCK/AMOUNT",
          "operator": "<",
          "value": "5",
        }
      ]
    },
    {
      "operation": "change-value",
      "element": "SHOP/SHOPITEM/STOCK/AMOUNT",
      "newValue": "300",
      "conditions": [
        {
          "element": "SHOP/SHOPITEM/STOCK/AMOUNT",
          "operator": "==",
          "value": "50",
          "nextCondition": "and"
        },
        {
          "element": "SHOP/SHOPITEM/CATEGORIES/CATEGORY",
          "operator": "contains",
          "value": "Spotřební materiál &gt; Papír"
        }
      ]
    }
  ]
}